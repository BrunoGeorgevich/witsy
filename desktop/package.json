    "@mistralai/mistralai": "^0.1.3",
    "applescript": "^1.0.0",
    "bootstrap-icons-vue": "^1.11.3",
    "cross-env": "^7.0.3",
    "electron-dl": "^3.5.2",
    "electron-log": "^5.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.2.0",
    "highlight.js": "^11.9.0",
    "html-to-text": "^9.0.5",
    "katex": "^0.16.10",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.1",
